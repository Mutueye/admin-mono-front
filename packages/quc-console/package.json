{
  "name": "quc-console",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@7.18.1",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:prod": "vite --mode prod",
    "dev:local": "vite --mode local",
    "build": "vue-tsc --noEmit && vite build --emptyOutDir --mode prod",
    "preview": "vite preview",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "npx lint-staged",
    "lint:es": "eslint --ext .js,.ts,.vue --fix src/",
    "lint:es:check": "eslint --ext .js,.ts,.vue src/",
    "lint:style": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "lint:style:check": "stylelint ./src/**/*.{vue,css,scss}"
  },
  "dependencies": {
    "@itshixun/qst-request-lib": "^1.0.1",
    "@itshixun/qst-ui-system": "^0.1.9",
    "@qst-admin/auth": "workspace:^",
    "@qst-admin/components": "workspace:^",
    "@qst-admin/composables": "workspace:^",
    "@qst-admin/layout": "workspace:^",
    "@qst-admin/request": "workspace:^",
    "@qst-admin/utils": "workspace:^",
    "@vueuse/core": "^10.5.0",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.5.1",
    "element-plus": "^2.4.0",
    "js-base64": "^3.7.5",
    "jsencrypt": "^3.3.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persist": "^1.0.0",
    "vanilla-colorful": "^0.7.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@iconify-json/line-md": "^1.1.32",
    "@iconify-json/mdi": "^1.1.54",
    "@qst-admin/eslint-config": "workspace:^",
    "@qst-admin/prettier-config": "workspace:^",
    "@qst-admin/ts-config": "workspace:^",
    "@qst-admin/types": "workspace:^",
    "@qst-admin/uno-config": "workspace:^",
    "@qst-admin/vite-config": "workspace:^",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.8.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.3",
    "sass": "^1.69.3",
    "stylelint": "^15.10.3",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-scss": "^5.2.1",
    "typescript": "^5.2.2",
    "unocss": "^0.56.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.11",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.19"
  },
  "lint-staged": {
    "./**/*.{vue,js,ts,jsx,tsx}": "eslint --fix",
    "./**/*.{vue,js,ts,jsx,tsx,md,json,scss,css,html}": "prettier . --write",
    "./**/*.{scss,css,vue}": "stylelint --fix"
  }
}
